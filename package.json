{
  "name": "devrahimi-tanstack-start-template",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "start": "node .output/server/index.mjs",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --config ./eslint.config.mjs",
    "format": "prettier . --write --config-precedence prefer-file ./prettier.config.mjs",
    "db:push": "drizzle-kit push --config ./src/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config ./src/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./src/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./src/drizzle.config.ts",
    "db:wipe": "rm -rf ./sqlite.db && touch ./sqlite.db",
    "prepare": "husky"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@t3-oss/env-core": "^0.12.0",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-form": "^1.0.0",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.66.5",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.130.2",
    "@tanstack/react-router-with-query": "^1.130.2",
    "@tanstack/react-start": "^1.130.2",
    "@tanstack/react-table": "^8.21.2",
    "@tanstack/router-plugin": "^1.121.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.476.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.0.2",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tanstack/eslint-config": "^0.1.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.3.4",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0",
    "vite": "^6.3.5"
  }
}
